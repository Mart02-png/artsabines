{"ast":null,"code":"import DataTable from 'datatables.net-vue3';\nimport DataTablesCore from 'datatables.net-bs5';\nimport axios from 'axios';\nDataTable.use(DataTablesCore);\nimport { ref } from 'vue';\nexport default {\n  components: {},\n  data: function () {\n    return {};\n  },\n  mounted() {\n    this.fetchEvents();\n  },\n  methods: {\n    fetchEvents() {\n      let tableData = ref([]);\n      axios.get('http://localhost:8000/api/events').then(response => {\n        let eventsFromAPI = response.data.filter(event => event.user === 'clientApproval').map(event => {\n          return {\n            id: event.id,\n            title: event.title,\n            email: event.email,\n            start: event.start,\n            end: event.end\n          };\n        });\n        tableData.value = [...eventsFromAPI];\n        console.log(tableData.value);\n      }).catch(error => {\n        console.error('Error fetching data:', error);\n      });\n    },\n    acceptEvent(id) {\n      axios.put(`http://localhost:8000/api/events/${id}`, {\n        user: 'accepted'\n      }).then(response => {\n        console.log(\"Event accepted:\", response.data);\n        // Assuming you want to update the local data after accepting\n        this.fetchEvents();\n      }).catch(error => {\n        console.error('Error accepting event:', error);\n      });\n    },\n    deleteEvent(id) {\n      axios.delete(`http://localhost:8000/api/events/${id}`).then(response => {\n        console.log(\"Event deleted:\", response.data);\n        // Assuming you want to update the local data after deleting\n        this.fetchEvents();\n      }).catch(error => {\n        console.error('Error deleting event:', error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["DataTable","DataTablesCore","axios","use","ref","components","data","mounted","fetchEvents","methods","tableData","get","then","response","eventsFromAPI","filter","event","user","map","id","title","email","start","end","value","console","log","catch","error","acceptEvent","put","deleteEvent","delete"],"sources":["C:\\Users\\Asus\\Desktop\\Clinic\\Working System\\clinicsystem\\src\\views\\AboutView.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <h1>Bootstrap 5 styled DataTable</h1>\n    <h2>DataTables + Vue3 example</h2>\n    <p>\n      In this example we show a DataTable in a Vue3 application being styled by\n      Bootstrap 5. DataTables supports a number of other styling libraries as\n      well, such as Bulma, Foundation, Boostrap 3, 4 and others.\n    </p>\n\n    <DataTable\n      :columns=\"columns\"\n      :data=\"tableData\"\n      class=\"table table-hover table-striped\"\n      width=\"100%\"\n    >\n      <thead>\n        <tr>\n          <th>ID</th>\n          <th>Title</th>\n          <th>Email</th>\n          <th>Start Date</th>\n          <th>End Date</th>\n          <th>Action</th>\n        </tr>\n      </thead>\n    </DataTable>\n  </div>\n</template>\n\n<script>\nimport DataTable from 'datatables.net-vue3';\nimport DataTablesCore from 'datatables.net-bs5';\nimport axios from 'axios';\nDataTable.use(DataTablesCore);\nimport { ref } from 'vue';\n\n\n\nexport default {\n  components: {\n  },\n\n  data: function () {\n    return {\n\n    }\n  },\n  mounted() {\n    this.fetchEvents();\n    \n  },\n\n  methods: {\n\n    fetchEvents(){\n      let tableData = ref([]);\n\n      axios.get('http://localhost:8000/api/events')\n        .then(response => {\n          let eventsFromAPI = response.data.filter(event => event.user === 'clientApproval').map(event => {\n            return {\n              id: event.id,\n              title: event.title,\n              email: event.email,\n              start: event.start,\n              end: event.end,\n            };\n          });\n          tableData.value = [...eventsFromAPI];\n          console.log(tableData.value);\n        })\n        .catch(error => {\n          console.error('Error fetching data:', error);\n        });\n    },\n    acceptEvent(id){\n      axios.put(`http://localhost:8000/api/events/${id}`, { user: 'accepted' })\n      .then(response => {\n        console.log(\"Event accepted:\", response.data);\n        // Assuming you want to update the local data after accepting\n        this.fetchEvents();\n      })\n      .catch(error => {\n        console.error('Error accepting event:', error);\n      });\n    },\n\n    deleteEvent(id){\n      axios.delete(`http://localhost:8000/api/events/${id}`)\n      .then(response => {\n        console.log(\"Event deleted:\", response.data);\n        // Assuming you want to update the local data after deleting\n        this.fetchEvents();\n      })\n      .catch(error => {\n        console.error('Error deleting event:', error);\n      });\n    }\n\n  }\n}\n</script>\n\n\n<style>\n@import 'bootstrap';\n@import 'datatables.net-bs5';\n</style>\n"],"mappings":"AA+BA,OAAOA,SAAQ,MAAO,qBAAqB;AAC3C,OAAOC,cAAa,MAAO,oBAAoB;AAC/C,OAAOC,KAAI,MAAO,OAAO;AACzBF,SAAS,CAACG,GAAG,CAACF,cAAc,CAAC;AAC7B,SAASG,GAAE,QAAS,KAAK;AAIzB,eAAe;EACbC,UAAU,EAAE,CACZ,CAAC;EAEDC,IAAI,EAAE,SAAAA,CAAA,EAAY;IAChB,OAAO,CAEP;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,WAAW,CAAC,CAAC;EAEpB,CAAC;EAEDC,OAAO,EAAE;IAEPD,WAAWA,CAAA,EAAE;MACX,IAAIE,SAAQ,GAAIN,GAAG,CAAC,EAAE,CAAC;MAEvBF,KAAK,CAACS,GAAG,CAAC,kCAAkC,EACzCC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAIC,aAAY,GAAID,QAAQ,CAACP,IAAI,CAACS,MAAM,CAACC,KAAI,IAAKA,KAAK,CAACC,IAAG,KAAM,gBAAgB,CAAC,CAACC,GAAG,CAACF,KAAI,IAAK;UAC9F,OAAO;YACLG,EAAE,EAAEH,KAAK,CAACG,EAAE;YACZC,KAAK,EAAEJ,KAAK,CAACI,KAAK;YAClBC,KAAK,EAAEL,KAAK,CAACK,KAAK;YAClBC,KAAK,EAAEN,KAAK,CAACM,KAAK;YAClBC,GAAG,EAAEP,KAAK,CAACO;UACb,CAAC;QACH,CAAC,CAAC;QACFb,SAAS,CAACc,KAAI,GAAI,CAAC,GAAGV,aAAa,CAAC;QACpCW,OAAO,CAACC,GAAG,CAAChB,SAAS,CAACc,KAAK,CAAC;MAC9B,CAAC,EACAG,KAAK,CAACC,KAAI,IAAK;QACdH,OAAO,CAACG,KAAK,CAAC,sBAAsB,EAAEA,KAAK,CAAC;MAC9C,CAAC,CAAC;IACN,CAAC;IACDC,WAAWA,CAACV,EAAE,EAAC;MACbjB,KAAK,CAAC4B,GAAG,CAAE,oCAAmCX,EAAG,EAAC,EAAE;QAAEF,IAAI,EAAE;MAAW,CAAC,EA