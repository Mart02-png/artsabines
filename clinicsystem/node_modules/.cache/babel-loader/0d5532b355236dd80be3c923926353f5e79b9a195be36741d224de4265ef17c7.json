{"ast":null,"code":"import { ref, onMounted } from 'vue';\nimport axios from 'axios';\nimport DataTable from 'vue3-datatable';\nexport default {\n  components: {\n    DataTable\n  },\n  setup() {\n    const events = ref([]);\n    const columns = [{\n      label: 'ID',\n      field: 'id'\n    }, {\n      label: 'Title',\n      field: 'title'\n    }, {\n      label: 'Email',\n      field: 'email'\n    }, {\n      label: 'Start',\n      field: 'start'\n    }, {\n      label: 'End',\n      field: 'end'\n    }];\n    onMounted(async () => {\n      try {\n        const {\n          data\n        } = await axios.get('http://localhost:8000/api/events');\n        events.value = data.filter(event => event.user === 'admin').map(event => ({\n          id: event.id,\n          title: event.title,\n          email: event.email,\n          start: event.start,\n          end: event.end\n        }));\n      } catch (error) {\n        console.error(error);\n      }\n    });\n    return {\n      events,\n      columns\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","axios","DataTable","components","setup","events","columns","label","field","data","get","value","filter","event","user","map","id","title","email","start","end","error","console"],"sources":["C:\\Users\\Asus\\Desktop\\Clinic\\Working System\\clinicsystem\\src\\views\\AboutView.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <h1>Bootstrap 5 styled DataTable</h1>\n    <h2>DataTables + Vue3 example</h2>\n    <p>\n      In this example we show a DataTable in a Vue3 application being styled by\n      Bootstrap 5. DataTables supports a number of other styling libraries as\n      well, such as Bulma, Foundation, Boostrap 3, 4 and others.\n    </p>\n\n    <DataTable :columns=\"columns\" :data=\"events\" />\n  </div>\n</template>\n\n<script>\nimport { ref, onMounted } from 'vue';\nimport axios from 'axios';\nimport DataTable from 'vue3-datatable';\n\nexport default {\n  components: {\n    DataTable\n  },\n  setup() {\n    const events = ref([]);\n    const columns = [\n      { label: 'ID', field: 'id' },\n      { label: 'Title', field: 'title' },\n      { label: 'Email', field: 'email' },\n      { label: 'Start', field: 'start' },\n      { label: 'End', field: 'end' }\n    ];\n\n    onMounted(async () => {\n      try {\n        const { data } = await axios.get('http://localhost:8000/api/events');\n        events.value = data.filter(event => event.user === 'admin').map(event => ({\n          id: event.id,\n          title: event.title,\n          email: event.email,\n          start: event.start,\n          end: event.end,\n        }));\n      } catch (error) {\n        console.error(error);\n      }\n    });\n\n    return { events, columns };\n  }\n};\n</script>\n\n<style scoped>\n@import 'bootstrap/dist/css/bootstrap.css';\n@import 'vue3-datatable/dist/vue3-datatable.css';\n</style>"],"mappings":"AAeA,SAASA,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAOC,SAAQ,MAAO,gBAAgB;AAEtC,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIN,GAAG,CAAC,EAAE,CAAC;IACtB,MAAMO,OAAM,GAAI,CACd;MAAEC,KAAK,EAAE,IAAI;MAAEC,KAAK,EAAE;IAAK,CAAC,EAC5B;MAAED,KAAK,EAAE,OAAO;MAAEC,KAAK,EAAE;IAAQ,CAAC,EAClC;MAAED,KAAK,EAAE,OAAO;MAAEC,KAAK,EAAE;IAAQ,CAAC,EAClC;MAAED,KAAK,EAAE,OAAO;MAAEC,KAAK,EAAE;IAAQ,CAAC,EAClC;MAAED,KAAK,EAAE,KAAK;MAAEC,KAAK,EAAE;IAAM,EAC9B;IAEDR,SAAS,CAAC,YAAY;MACpB,IAAI;QACF,MAAM;UAAES;QAAK,IAAI,MAAMR,KAAK,CAACS,GAAG,CAAC,kCAAkC,CAAC;QACpEL,MAAM,CAACM,KAAI,GAAIF,IAAI,CAACG,MAAM,CAACC,KAAI,IAAKA,KAAK,CAACC,IAAG,KAAM,OAAO,CAAC,CAACC,GAAG,CAACF,KAAI,KAAM;UACxEG,EAAE,EAAEH,KAAK,CAACG,EAAE;UACZC,KAAK,EAAEJ,KAAK,CAACI,KAAK;UAClBC,KAAK,EAAEL,KAAK,CAACK,KAAK;UAClBC,KAAK,EAAEN,KAAK,CAACM,KAAK;UAClBC,GAAG,EAAEP,KAAK,CAACO;QACb,CAAC,CAAC,CAAC;MACL,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;MACtB;IACF,CAAC,CAAC;IAEF,OAAO;MAAEhB,MAAM;MAAEC;IAAQ,CAAC;EAC5B;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"modul