{"ast":null,"code":"import DataTable from 'datatables.net-vue3';\nimport DataTablesCore from 'datatables.net-bs5';\nDataTable.use(DataTablesCore);\nimport Buttons from 'datatables.net-buttons';\nDataTable.use(Buttons);\nimport axios from 'axios';\nimport * as bootstrap from 'bootstrap';\nexport default {\n  components: {\n    DataTable\n  },\n  data() {\n    return {\n      deletemodal: null,\n      columns: [{\n        data: 'id'\n      }, {\n        data: 'title'\n      }, {\n        data: 'email'\n      }, {\n        data: 'start'\n      }, {\n        data: 'end'\n      }, {\n        // New action column definition\n        data: null,\n        render: row => {\n          return `\n            <button class=\"btn btn-success btn-sm\" @click=\"acceptEvent(${row.id})\">Accept</button>\n            <button class=\"btn btn-danger btn-sm\" @click=\"deleteEvent(${row.id})\">Delete</button>\n   \n            `;\n        }\n      }],\n      tableData: []\n    };\n  },\n  mounted() {\n    this.fetchEvents();\n    this.deletemodal = new bootstrap.Modal(document.getElementById('deleteModal'), {\n      // Optional: specify options here\n    });\n  },\n  methods: {\n    fetchEvents() {\n      axios.get('http://localhost:8000/api/events').then(response => {\n        let eventsFromAPI = response.data.filter(event => event.user === 'clientApproval').map(event => {\n          return {\n            id: event.id,\n            title: event.title,\n            email: event.email,\n            start: event.start,\n            end: event.end\n          };\n        });\n        this.tableData = [...eventsFromAPI];\n      }).catch(error => {\n        console.error('Error fetching data:', error);\n      });\n    },\n    acceptEvent(id) {\n      console.log('Accepting event with ID:', id);\n      // Implement your logic here\n    },\n    deleteEvent(id) {\n      console.log('Deleting event with ID:', id);\n      // Implement your logic here\n    },\n    deleteItemModal() {\n      console.log('Deleted:');\n      this.deleteEvent.hide;\n    }\n  }\n};","map":{"version":3,"names":["DataTable","DataTablesCore","use","Buttons","axios","bootstrap","components","data","deletemodal","columns","render","row","id","tableData","mounted","fetchEvents","Modal","document","getElementById","methods","get","then","response","eventsFromAPI","filter","event","user","map","title","email","start","end","catch","error","console","acceptEvent","log","deleteEvent","deleteItemModal","hide"],"sources":["C:\\Users\\Asus\\Desktop\\Clinic\\Working System\\clinicsystem\\src\\views\\AboutView.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <h1>Bootstrap 5 styled DataTable</h1>\n    <h2>DataTables + Vue3 example</h2>\n    <p>\n      In this example we show a DataTable in a Vue3 application being styled by\n      Bootstrap 5. DataTables supports a number of other styling libraries as\n      well, such as Bulma, Foundation, Boostrap 3, 4 and others.\n    </p>\n\n    <DataTable\n      :columns=\"columns\"\n      :data=\"tableData\"\n      class=\"table table-hover table-striped\"\n      width=\"100%\"\n    >\n      <thead>\n        <tr>\n          <th>ID</th>\n          <th>Title</th>\n          <th>Email</th>\n          <th>Start Date</th>\n          <th>End Date</th>\n          <th>Action</th>\n        </tr>\n      </thead>\n    </DataTable>\n    <button class=\"btn btn-danger btn-sm\" @click=\"deleteEvent()\">Delete</button>\n          <!-- Delete -->\n          <div class=\"modal fade\" id=\"deleteModal\" tabindex=\"-1\" aria-labelledby=\"deleteModal\" aria-hidden=\"true\">\n        <div class=\"modal-dialog modal-dialog-centered\">\n          <div class=\"modal-content\">\n            <div class=\"modal-header bg-danger text-white\">\n              <h5 class=\"modal-title\">Delete</h5>\n              <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\n            </div>\n            <div class=\"modal-body\">\n              <p>Are you sure you want to delete?</p>\n            </div>\n            <div class=\"modal-footer\">\n              <button type=\"button\" class=\"btn btn-secondary\" data-bs-dismiss=\"modal\">Close</button>\n              <button type=\"button\" class=\"btn btn-danger\" @click=\"deleteItemModal()\">Delete</button>\n            </div>\n          </div>\n        </div>\n      </div>\n  </div>\n</template>\n\n<script>\nimport DataTable from 'datatables.net-vue3';\nimport DataTablesCore from 'datatables.net-bs5';\nDataTable.use(DataTablesCore);\nimport Buttons from 'datatables.net-buttons';\nDataTable.use(Buttons);\nimport axios from 'axios';\nimport * as bootstrap from 'bootstrap';\n\n\n\nexport default {\n  components: {\n    DataTable,\n  },\n\n  data() {\n    return {\n      deletemodal: null,\n      columns: [\n        { data: 'id' },\n        { data: 'title' },\n        { data: 'email' },\n        { data: 'start' },\n        { data: 'end' },\n        {\n          // New action column definition\n          data: null,\n          render: (row) => {\n            return `\n            <button class=\"btn btn-success btn-sm\" @click=\"acceptEvent(${row.id})\">Accept</button>\n            <button class=\"btn btn-danger btn-sm\" @click=\"deleteEvent(${row.id})\">Delete</button>\n   \n            `;\n          }\n        }\n      ],\n      tableData: [], \n    }\n  },\n  mounted() {\n    this.fetchEvents();   \n    this.deletemodal = new bootstrap.Modal(document.getElementById('deleteModal'), {\n      // Optional: specify options here\n    }); \n  },\n\n  methods: {\n    fetchEvents(){\n      axios.get('http://localhost:8000/api/events')\n        .then(response => {\n          let eventsFromAPI = response.data.filter(event => event.user === 'clientApproval').map(event => {\n            return {\n              id: event.id,\n              title: event.title,\n              email: event.email,\n              start: event.start,\n              end: event.end,\n            };\n          });\n          this.tableData = [...eventsFromAPI];\n        })\n        .catch(error => {\n          console.error('Error fetching data:', error);\n        });\n    },\n    acceptEvent(id) {\n    console.log('Accepting event with ID:', id);\n    // Implement your logic here\n},\ndeleteEvent(id) {\n    console.log('Deleting event with ID:', id);\n    // Implement your logic here\n},\n    deleteItemModal(){\n      console.log('Deleted:');\n      this.deleteEvent.hide;\n    }\n\n  }\n}\n</script>\n\n\n<style>\n@import 'bootstrap';\n@import 'datatables.net-bs5';\n</style>\n"],"mappings":"AAkDA,OAAOA,SAAQ,MAAO,qBAAqB;AAC3C,OAAOC,cAAa,MAAO,oBAAoB;AAC/CD,SAAS,CAACE,GAAG,CAACD,cAAc,CAAC;AAC7B,OAAOE,OAAM,MAAO,wBAAwB;AAC5CH,SAAS,CAACE,GAAG,CAACC,OAAO,CAAC;AACtB,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAO,KAAKC,SAAQ,MAAO,WAAW;AAItC,eAAe;EACbC,UAAU,EAAE;IACVN;EACF,CAAC;EAEDO,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,IAAI;MACjBC,OAAO,EAAE,CACP;QAAEF,IAAI,EAAE;MAAK,CAAC,EACd;QAAEA,IAAI,EAAE;MAAQ,CAAC,EACjB;QAAEA,IAAI,EAAE;MAAQ,CAAC,EACjB;QAAEA,IAAI,EAAE;MAAQ,CAAC,EACjB;QAAEA,IAAI,EAAE;MAAM,CAAC,EACf;QACE;QACAA,IAAI,EAAE,IAAI;QACVG,MAAM,EAAGC,GAAG,IAAK;UACf,OAAO;yEACsDA,GAAG,CAACC,EAAG;wEACRD,GAAG,CAACC,EAAG;;aAElE;QACH;MACF,EACD;MACDC,SAAS,EAAE;IACb;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,WAAW,CAAC,CAAC;IAClB,IAAI,CAACP,WAAU,GAAI,IAAIH,SAAS,CAACW,KAAK,CAACC,QAAQ,CAACC,cAAc,CAAC,aAAa,CAAC,EAAE;MAC7E;IAAA,CACD,CAAC;EACJ,CAAC;EAEDC,OAAO,EAAE;IACPJ,WAAWA,CAAA,EAAE;MACXX,KAAK,CAACgB,GAAG,CAAC,kCAAkC,EACzCC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAIC,aAAY,GAAID,QAAQ,CAACf,IAAI,CAACiB,MAAM,CAACC,KAAI,IAAKA,KAAK,CAACC,IAAG,KAAM,gBAAgB,CAAC,CAACC,GAAG,CAACF,KAAI,IAAK;UAC9F,OAAO;YACLb,EAAE,EAAEa,KAAK,CAACb,EAAE;YACZgB,KAAK,EAAEH,KAAK,CAACG,KAAK;YAClBC,KAAK,EAAEJ,KAAK,CAACI,KAAK;YAClBC,KAAK,EAAEL,KAAK,CAACK,KAAK;YAClBC,GAAG,EAAEN,KAAK,CAACM;UACb,CAAC;QACH,CAAC,CAAC;QACF,IAAI,CAAClB,SAAQ,GAAI,CAAC,GAAGU,aAAa,CAAC;MACrC,CAAC,EACAS,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,C