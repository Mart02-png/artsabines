{"ast":null,"code":"import DataTable from 'datatables.net-vue3';\nimport DataTablesCore from 'datatables.net-bs5';\nimport axios from 'axios';\nDataTable.use(DataTablesCore);\nimport { ref } from 'vue';\nexport default {\n  components: {},\n  data: function () {\n    return {\n      columns: [{\n        data: 'id'\n      }, {\n        data: 'title'\n      }, {\n        data: 'email'\n      }, {\n        data: 'start'\n      }, {\n        data: 'end'\n      }, {\n        // New action column definition\n        data: null,\n        render: (data, type, row) => {\n          return `\n              <button class=\"btn btn-success btn-sm\" @click=\"acceptEvent(${row.id})\">Accept</button>\n              <button class=\"btn btn-danger btn-sm\" @click=\"deleteEvent(${row.id})\">Delete</button>\n            `;\n        }\n      }],\n      tableData: []\n    };\n  },\n  mounted() {\n    this.fetchEvents();\n  },\n  methods: {\n    fetchEvents() {\n      let tableData = ref([]);\n      axios.get('http://localhost:8000/api/events').then(response => {\n        let eventsFromAPI = response.data.filter(event => event.user === 'clientApproval').map(event => {\n          return {\n            id: event.id,\n            title: event.title,\n            email: event.email,\n            start: event.start,\n            end: event.end\n          };\n        });\n        tableData.value = [...eventsFromAPI];\n        console.log(tableData.value);\n      }).catch(error => {\n        console.error('Error fetching data:', error);\n      });\n    },\n    acceptEvent(id) {\n      axios.put(`http://localhost:8000/api/events/${id}`, {\n        user: 'accepted'\n      }).then(response => {\n        console.log(\"Event accepted:\", response.data);\n        // Assuming you want to update the local data after accepting\n        this.fetchEvents();\n      }).catch(error => {\n        console.error('Error accepting event:', error);\n      });\n    },\n    deleteEvent(id) {\n      axios.delete(`http://localhost:8000/api/events/${id}`).then(response => {\n        console.log(\"Event deleted:\", response.data);\n        // Assuming you want to update the local data after deleting\n        this.fetchEvents();\n      }).catch(error => {\n        console.error('Error deleting event:', error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["DataTable","DataTablesCore","axios","use","ref","components","data","columns","render","type","row","id","tableData","mounted","fetchEvents","methods","get","then","response","eventsFromAPI","filter","event","user","map","title","email","start","end","value","console","log","catch","error","acceptEvent","put","deleteEvent","delete"],"sources":["C:\\Users\\Asus\\Desktop\\Clinic\\Working System\\clinicsystem\\src\\views\\AboutView.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <h1>Bootstrap 5 styled DataTable</h1>\n    <h2>DataTables + Vue3 example</h2>\n    <p>\n      In this example we show a DataTable in a Vue3 application being styled by\n      Bootstrap 5. DataTables supports a number of other styling libraries as\n      well, such as Bulma, Foundation, Boostrap 3, 4 and others.\n    </p>\n\n    <DataTable\n      :columns=\"columns\"\n      :data=\"tableData\"\n      class=\"table table-hover table-striped\"\n      width=\"100%\"\n    >\n      <thead>\n        <tr>\n          <th>ID</th>\n          <th>Title</th>\n          <th>Email</th>\n          <th>Start Date</th>\n          <th>End Date</th>\n          <th>Action</th>\n        </tr>\n      </thead>\n    </DataTable>\n  </div>\n</template>\n\n<script>\nimport DataTable from 'datatables.net-vue3';\nimport DataTablesCore from 'datatables.net-bs5';\nimport axios from 'axios';\nDataTable.use(DataTablesCore);\nimport { ref } from 'vue';\n\n\n\n\nexport default {\n  components: {\n  },\n\n  data: function () {\n    return {\n      columns: [\n        { data: 'id' },\n        { data: 'title' },\n        { data: 'email' },\n        { data: 'start' },\n        { data: 'end' },\n        {\n          // New action column definition\n          data: null,\n          render: (data, type, row) => {\n            return `\n              <button class=\"btn btn-success btn-sm\" @click=\"acceptEvent(${row.id})\">Accept</button>\n              <button class=\"btn btn-danger btn-sm\" @click=\"deleteEvent(${row.id})\">Delete</button>\n            `;\n          }\n        }\n      ],\n      tableData: [], \n    }\n  },\n  mounted() {\n    this.fetchEvents();\n    \n  },\n\n  methods: {\n\n    fetchEvents(){\n      let tableData = ref([]);\n\n      axios.get('http://localhost:8000/api/events')\n        .then(response => {\n          let eventsFromAPI = response.data.filter(event => event.user === 'clientApproval').map(event => {\n            return {\n              id: event.id,\n              title: event.title,\n              email: event.email,\n              start: event.start,\n              end: event.end,\n            };\n          });\n          tableData.value = [...eventsFromAPI];\n          console.log(tableData.value);\n        })\n        .catch(error => {\n          console.error('Error fetching data:', error);\n        });\n    },\n    acceptEvent(id){\n      axios.put(`http://localhost:8000/api/events/${id}`, { user: 'accepted' })\n      .then(response => {\n        console.log(\"Event accepted:\", response.data);\n        // Assuming you want to update the local data after accepting\n        this.fetchEvents();\n      })\n      .catch(error => {\n        console.error('Error accepting event:', error);\n      });\n    },\n\n    deleteEvent(id){\n      axios.delete(`http://localhost:8000/api/events/${id}`)\n      .then(response => {\n        console.log(\"Event deleted:\", response.data);\n        // Assuming you want to update the local data after deleting\n        this.fetchEvents();\n      })\n      .catch(error => {\n        console.error('Error deleting event:', error);\n      });\n    }\n\n  }\n}\n</script>\n\n\n<style>\n@import 'bootstrap';\n@import 'datatables.net-bs5';\n</style>\n"],"mappings":"AA+BA,OAAOA,SAAQ,MAAO,qBAAqB;AAC3C,OAAOC,cAAa,MAAO,oBAAoB;AAC/C,OAAOC,KAAI,MAAO,OAAO;AACzBF,SAAS,CAACG,GAAG,CAACF,cAAc,CAAC;AAC7B,SAASG,GAAE,QAAS,KAAK;AAKzB,eAAe;EACbC,UAAU,EAAE,CACZ,CAAC;EAEDC,IAAI,EAAE,SAAAA,CAAA,EAAY;IAChB,OAAO;MACLC,OAAO,EAAE,CACP;QAAED,IAAI,EAAE;MAAK,CAAC,EACd;QAAEA,IAAI,EAAE;MAAQ,CAAC,EACjB;QAAEA,IAAI,EAAE;MAAQ,CAAC,EACjB;QAAEA,IAAI,EAAE;MAAQ,CAAC,EACjB;QAAEA,IAAI,EAAE;MAAM,CAAC,EACf;QACE;QACAA,IAAI,EAAE,IAAI;QACVE,MAAM,EAAEA,CAACF,IAAI,EAAEG,IAAI,EAAEC,GAAG,KAAK;UAC3B,OAAO;2EACwDA,GAAG,CAACC,EAAG;0EACRD,GAAG,CAACC,EAAG;aACpE;QACH;MACF,EACD;MACDC,SAAS,EAAE;IACb;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,WAAW,CAAC,CAAC;EAEpB,CAAC;EAEDC,OAAO,EAAE;IAEPD,WAAWA,CAAA,EAAE;MACX,IAAIF,SAAQ,GAAIR,GAAG,CAAC,EAAE,CAAC;MAEvBF,KAAK,CAACc,GAAG,CAAC,kCAAkC,EACzCC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAIC,aAAY,GAAID,QAAQ,CAACZ,IAAI,CAACc,MAAM,CAACC,KAAI,IAAKA,KAAK,CAACC,IAAG,KAAM,gBAAgB,CAAC,CAACC,GAAG,CAACF,KAAI,IAAK;UAC9F,OAAO;YACLV,EAAE,EAAEU,KAAK,CAACV,EAAE;YACZa,KAAK,EAAEH,KAAK,CAACG,KAAK;YAClBC,KAAK,EAAEJ,KAAK,CAACI,KAAK;YAClBC,KAAK,EAAEL,KAAK,CAACK,KAAK;YAClBC,GAAG,EAAEN,KAAK,CAACM;UACb,CAAC;QACH,CAAC,CAAC;QACFf,SAAS,CAACgB,KAAI,GAAI,CAAC,GAAGT,aAAa,CAAC;QACpCU,OAAO,CAACC,GAAG,CAAClB,SAAS,CAACgB,KAAK,CAAC;MAC9B,CAAC,EACAG,KAAK,CAACC,KAAI,IAAK;QACdH,OAAO,CAACG,KAAK,CAAC,sBAAsB,EAAEA,KAAK,CAAC;MAC9C,CAAC,CAAC;IACN,CAAC;IACDC,WAAWA,CAACtB,EAAE,EAAC;MACbT,KAAK,CAACgC,GAAG,CAAE,oCAAmCvB,EAAG,EAAC,EAAE;QAAEW,IAAI,EAAE;MAAW,CAAC,EACvEL,IAAI,CAACC,QAAO,IAAK;QAChBW,OAAO,CAACC,GAAG,CAAC,iBAAiB,EAAEZ,QAAQ,CAACZ,IAAI,CAAC;QAC7C;QACA,IAAI,CAACQ,WAAW,CAAC,CAAC;MACpB,CAAC,EACAiB,KAAK,CAACC,KAAI,IAAK;QACdH,OAAO,CAACG,KAAK,CAAC,wBAAwB,EAAEA,KAAK,CAAC;MAChD,CAAC,CAAC;IACJ,CAAC;IAEDG,WAAWA,CAACxB,EAAE,EAAC;MACbT,KAAK,CAACkC,MAAM,CAAE,oCAAmCzB,EAAG,EAAC,EACpDM,IAAI,CAACC,QAAO,IAAK;QAChBW,OAAO,CAACC,GAAG,CAAC,gBAAgB,EAAEZ,QAAQ,CAACZ,IAAI,CAAC;QAC5C;QACA,IAAI,CAACQ,WAAW,CAAC,CAAC;MA