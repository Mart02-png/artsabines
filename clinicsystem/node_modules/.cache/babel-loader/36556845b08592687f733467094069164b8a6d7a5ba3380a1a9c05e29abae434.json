{"ast":null,"code":"import axios from 'axios';\nimport 'datatables.net-dt/css/jquery.dataTables.css';\nimport 'datatables.net-dt/js/dataTables.dataTables';\naxios.get('http://localhost:8000/api/events').then(response => {\n  let eventsFromAPI = response.data.filter(event => event.user === 'admin').map(event => {\n    return {\n      id: event.id,\n      title: event.title,\n      email: event.email,\n      user: event.user,\n      start: event.start\n    };\n  });\n\n  // Create the datatable\n  $('#eventsTable').DataTable({\n    data: eventsFromAPI,\n    columns: [{\n      data: 'id'\n    }, {\n      data: 'title'\n    }, {\n      data: 'email'\n    }, {\n      data: 'user'\n    }, {\n      data: 'start'\n    }]\n  });\n}).catch(error => {\n  console.error(error);\n});","map":{"version":3,"names":["axios","get","then","response","eventsFromAPI","data","filter","event","user","map","id","title","email","start","$","DataTable","columns","catch","error","console"],"sources":["C:\\Users\\Asus\\Desktop\\Clinic\\Working System\\clinicsystem\\src\\views\\AboutView.vue"],"sourcesContent":["<template>\n  <div>\n    <h1>THIS IS ABOUT</h1>\n    <!-- Button to trigger the modal -->\n    <button type=\"button\" class=\"btn btn-primary\" @click=\"showModal = true\">\n      Launch Modal\n    </button>\n\n    <!-- Modal -->\n    <div class=\"modal\" tabindex=\"-1\" role=\"dialog\" :class=\"{ 'show': showModal }\">\n      <div class=\"modal-dialog\" role=\"document\">\n        <div class=\"modal-content\">\n          <div class=\"modal-header\">\n            <h5 class=\"modal-title\">Modal title</h5>\n            <button type=\"button\" class=\"close\" @click=\"showModal = false\"> <!-- Change showModal to false here -->\n              <span aria-hidden=\"true\">&times;</span>\n            </button>\n          </div>\n          <div class=\"modal-body\">\n            <p>Modal body text goes here.</p>\n          </div>\n          <div class=\"modal-footer\">\n            <button type=\"button\" class=\"btn btn-secondary\" @click=\"showModal = false\">Close</button>\n            <button type=\"button\" class=\"btn btn-primary\">Save changes</button>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\nimport 'datatables.net-dt/css/jquery.dataTables.css';\nimport 'datatables.net-dt/js/dataTables.dataTables';\n\naxios.get('http://localhost:8000/api/events')\n  .then(response => {\n    let eventsFromAPI = response.data.filter(event => event.user === 'admin').map(event => {\n      return {\n        id: event.id,\n        title: event.title,\n        email: event.email,\n        user: event.user,\n        start: event.start,\n      };\n    });\n\n    // Create the datatable\n    $('#eventsTable').DataTable({\n      data: eventsFromAPI,\n      columns: [\n        { data: 'id' },\n        { data: 'title' },\n        { data: 'email' },\n        { data: 'user' },\n        { data: 'start' },\n      ],\n    });\n  })\n  .catch(error => {\n    console.error(error);\n  });\n</script>\n\n<style>\n/* You may add custom styling for your modal here */\n</style>\n\n\n\n\n"],"mappings":"AAgCA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAO,6CAA6C;AACpD,OAAO,4CAA4C;AAEnDA,KAAK,CAACC,GAAG,CAAC,kCAAkC,EACzCC,IAAI,CAACC,QAAO,IAAK;EAChB,IAAIC,aAAY,GAAID,QAAQ,CAACE,IAAI,CAACC,MAAM,CAACC,KAAI,IAAKA,KAAK,CAACC,IAAG,KAAM,OAAO,CAAC,CAACC,GAAG,CAACF,KAAI,IAAK;IACrF,OAAO;MACLG,EAAE,EAAEH,KAAK,CAACG,EAAE;MACZC,KAAK,EAAEJ,KAAK,CAACI,KAAK;MAClBC,KAAK,EAAEL,KAAK,CAACK,KAAK;MAClBJ,IAAI,EAAED,KAAK,CAACC,IAAI;MAChBK,KAAK,EAAEN,KAAK,CAACM;IACf,CAAC;EACH,CAAC,CAAC;;EAEF;EACAC,CAAC,CAAC,cAAc,CAAC,CAACC,SAAS,CAAC;IAC1BV,IAAI,EAAED,aAAa;IACnBY,OAAO,EAAE,CACP;MAAEX,IAAI,EAAE;IAAK,CAAC,EACd;MAAEA,IAAI,EAAE;IAAQ,CAAC,EACjB;MAAEA,IAAI,EAAE;IAAQ,CAAC,EACjB;MAAEA,IAAI,EAAE;IAAO,CAAC,EAChB;MAAEA,IAAI,EAAE;IAAQ,CAAC;EAErB,CAAC,CAAC;AACJ,CAAC,EACAY,KAAK,CAACC,KAAI,IAAK;EACdC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;AACtB,CAAC,CAAC","ignoreList":[]},"