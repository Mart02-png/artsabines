{"ast":null,"code":"import DataTable from 'datatables.net-vue3';\nimport DataTablesCore from 'datatables.net-bs5';\nimport Buttons from 'datatables.net-buttons/js/dataTables.buttons.min.js';\nimport 'datatables.net-buttons/js/buttons.html5.min.js';\nimport 'datatables.net-buttons/js/buttons.print.min.js';\nimport 'datatables.net-buttons-bs5/css/buttons.bootstrap5.min.css';\nimport axios from 'axios';\nimport * as bootstrap from 'bootstrap';\nexport default {\n  components: {\n    DataTable\n  },\n  data() {\n    return {\n      deletemodal: null,\n      columns: [{\n        data: 'id'\n      }, {\n        data: 'title'\n      }, {\n        data: 'email'\n      }, {\n        data: 'start'\n      }, {\n        data: 'end'\n      }, {\n        data: null,\n        render: () => {\n          return null; // Custom buttons will be added through DataTables API\n        }\n      }],\n      tableData: [],\n      dataTableInstance: null\n    };\n  },\n  mounted() {\n    this.fetchEvents();\n    this.initDataTable();\n    this.deletemodal = new bootstrap.Modal(document.getElementById('deleteModal'), {\n      // Optional: specify options here\n    });\n  },\n  methods: {\n    fetchEvents() {\n      axios.get('http://localhost:8000/api/events').then(response => {\n        let eventsFromAPI = response.data.filter(event => event.user === 'clientApproval').map(event => {\n          return {\n            id: event.id,\n            title: event.title,\n            email: event.email,\n            start: event.start,\n            end: event.end\n          };\n        });\n        this.tableData = [...eventsFromAPI];\n      }).catch(error => {\n        console.error('Error fetching data:', error);\n      });\n    },\n    initDataTable() {\n      this.$nextTick(() => {\n        this.dataTableInstance = $('#dataTable').DataTable({\n          dom: 'Bfrtip',\n          buttons: [{\n            text: 'Accept',\n            className: 'btn btn-success btn-sm',\n            action: () => {\n              // Handle accept button click\n              console.log('Accept button clicked');\n            }\n          }, {\n            text: 'Delete',\n            className: 'btn btn-danger btn-sm',\n            action: () => {\n              // Handle delete button click\n              console.log('Delete button clicked');\n              this.deletemodal.show();\n            }\n          }]\n        });\n      });\n    }\n  }\n};","map":{"version":3,"names":["DataTable","DataTablesCore","Buttons","axios","bootstrap","components","data","deletemodal","columns","render","tableData","dataTableInstance","mounted","fetchEvents","initDataTable","Modal","document","getElementById","methods","get","then","response","eventsFromAPI","filter","event","user","map","id","title","email","start","end","catch","error","console","$nextTick","$","dom","buttons","text","className","action","log","show"],"sources":["C:\\Users\\Asus\\Desktop\\Clinic\\Working System\\clinicsystem\\src\\views\\AboutView.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <h1>Bootstrap 5 styled DataTable</h1>\n    <h2>DataTables + Vue3 example</h2>\n    <p>\n      In this example we show a DataTable in a Vue3 application being styled by\n      Bootstrap 5. DataTables supports a number of other styling libraries as\n      well, such as Bulma, Foundation, Boostrap 3, 4 and others.\n    </p>\n\n    <DataTable\n      :columns=\"columns\"\n      :data=\"tableData\"\n      class=\"table table-hover table-striped\"\n      width=\"100%\"\n    >\n      <thead>\n        <tr>\n          <th>ID</th>\n          <th>Title</th>\n          <th>Email</th>\n          <th>Start Date</th>\n          <th>End Date</th>\n          <th>Action</th>\n        </tr>\n      </thead>\n    </DataTable>\n  </div>\n</template>\n\n<script>\nimport DataTable from 'datatables.net-vue3';\nimport DataTablesCore from 'datatables.net-bs5';\nimport Buttons from 'datatables.net-buttons/js/dataTables.buttons.min.js';\nimport 'datatables.net-buttons/js/buttons.html5.min.js';\nimport 'datatables.net-buttons/js/buttons.print.min.js';\nimport 'datatables.net-buttons-bs5/css/buttons.bootstrap5.min.css';\nimport axios from 'axios';\nimport * as bootstrap from 'bootstrap';\n\nexport default {\n  components: {\n    DataTable,\n  },\n\n  data() {\n    return {\n      deletemodal: null,\n      columns: [\n        { data: 'id' },\n        { data: 'title' },\n        { data: 'email' },\n        { data: 'start' },\n        { data: 'end' },\n        {\n          data: null,\n          render: () => {\n            return null; // Custom buttons will be added through DataTables API\n          }\n        }\n      ],\n      tableData: [],\n      dataTableInstance: null\n    }\n  },\n  mounted() {\n    this.fetchEvents();\n    this.initDataTable();   \n    this.deletemodal = new bootstrap.Modal(document.getElementById('deleteModal'), {\n      // Optional: specify options here\n    }); \n  },\n\n  methods: {\n    fetchEvents() {\n      axios.get('http://localhost:8000/api/events')\n        .then(response => {\n          let eventsFromAPI = response.data.filter(event => event.user === 'clientApproval').map(event => {\n            return {\n              id: event.id,\n              title: event.title,\n              email: event.email,\n              start: event.start,\n              end: event.end,\n            };\n          });\n          this.tableData = [...eventsFromAPI];\n        })\n        .catch(error => {\n          console.error('Error fetching data:', error);\n        });\n    },\n\n    initDataTable() {\n      this.$nextTick(() => {\n        this.dataTableInstance = $('#dataTable').DataTable({\n          dom: 'Bfrtip',\n          buttons: [\n            {\n              text: 'Accept',\n              className: 'btn btn-success btn-sm',\n              action: () => {\n                // Handle accept button click\n                console.log('Accept button clicked');\n              }\n            },\n            {\n              text: 'Delete',\n              className: 'btn btn-danger btn-sm',\n              action: () => {\n                // Handle delete button click\n                console.log('Delete button clicked');\n                this.deletemodal.show();\n              }\n            }\n          ]\n        });\n      });\n    }\n  }\n}\n</script>\n\n<style>\n@import 'bootstrap';\n@import 'datatables.net-bs5';\n@import 'datatables.net-buttons-bs5/css/buttons.bootstrap5.min.css';\n</style>\n"],"mappings":"AA+BA,OAAOA,SAAQ,MAAO,qBAAqB;AAC3C,OAAOC,cAAa,MAAO,oBAAoB;AAC/C,OAAOC,OAAM,MAAO,qDAAqD;AACzE,OAAO,gDAAgD;AACvD,OAAO,gDAAgD;AACvD,OAAO,2DAA2D;AAClE,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAO,KAAKC,SAAQ,MAAO,WAAW;AAEtC,eAAe;EACbC,UAAU,EAAE;IACVL;EACF,CAAC;EAEDM,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,IAAI;MACjBC,OAAO,EAAE,CACP;QAAEF,IAAI,EAAE;MAAK,CAAC,EACd;QAAEA,IAAI,EAAE;MAAQ,CAAC,EACjB;QAAEA,IAAI,EAAE;MAAQ,CAAC,EACjB;QAAEA,IAAI,EAAE;MAAQ,CAAC,EACjB;QAAEA,IAAI,EAAE;MAAM,CAAC,EACf;QACEA,IAAI,EAAE,IAAI;QACVG,MAAM,EAAEA,CAAA,KAAM;UACZ,OAAO,IAAI,EAAE;QACf;MACF,EACD;MACDC,SAAS,EAAE,EAAE;MACbC,iBAAiB,EAAE;IACrB;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,WAAW,CAAC,CAAC;IAClB,IAAI,CAACC,aAAa,CAAC,CAAC;IACpB,IAAI,CAACP,WAAU,GAAI,IAAIH,SAAS,CAACW,KAAK,CAACC,QAAQ,CAACC,cAAc,CAAC,aAAa,CAAC,EAAE;MAC7E;IAAA,CACD,CAAC;EACJ,CAAC;EAEDC,OAAO,EAAE;IACPL,WAAWA,CAAA,EAAG;MACZV,KAAK,CAACgB,GAAG,CAAC,kCAAkC,EACzCC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAIC,aAAY,GAAID,QAAQ,CAACf,IAAI,CAACiB,MAAM,CAACC,KAAI,IAAKA,KAAK,CAACC,IAAG,KAAM,gBAAgB,CAAC,CAACC,GAAG,CAACF,KAAI,IAAK;UAC9F,OAAO;YACLG,EAAE,EAAEH,KAAK,CAACG,EAAE;YACZC,KAAK,EAAEJ,KAAK,CAACI,KAAK;YAClBC,KAAK,EAAEL,KAAK,CAACK,KAAK;YAClBC,KAAK,EAAEN,KAAK,CAACM,KAAK;YAClBC,GAAG,EAAEP,KAAK,CAACO;UACb,CAAC;QACH,CAAC,CAAC;QACF,IAAI,CAACrB,SAAQ,GAAI,CAAC,GAAGY,aAAa,CAAC;MACrC,CAAC,EACAU,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,sBAAsB,EAAEA,KAAK,CAAC;MAC9C,CAAC,CAAC;IACN,CAAC;IAEDnB,aAAaA,CAAA,EAAG;MACd,IAAI,CAACqB,SAAS,CAAC,MA